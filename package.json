{
  "name": "oanda-api-ts",
  "version": "0.0.0",
  "description": "oanda-api-ts",
  "main": "build/src/index.js",
  "types": "build/src/index.d.ts",
  "files": [
    "build/src"
  ],
  "license": "Apache-2.0",
  "keywords": [
    "oanda-api-ts"
  ],
  "scripts": {
    "test": "mocha -r ts-node/register test/**/*.spec.ts",
    "check-src": "gts check src/**/*.ts",
    "check-test": "gts check test/**/*.ts",
    "check-all": "npm run check-src && npm run check-test",
    "clean": "gts clean",
    "compile": "tsc",
    "fix-src": "gts fix src/**/*.ts",
    "fix-test": "gts fix test/**/*.ts",
    "fix-all": "npm run fix-src && npm run fix-test",
    "prepare": "npm run compile",
    "pretest": "npm run compile",
    "posttest": "echo 'npm run check-all'"
  },
  "devDependencies": {
    "@types/chai": "^4.2.12",
    "@types/mocha": "^8.0.2",
    "@types/node": "^13.11.1",
    "chai": "^4.2.0",
    "gts": "^2.0.2",
    "mocha": "^8.1.1",
    "nyc": "^15.1.0",
    "ts-node": "^8.10.2",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "decimal.js": "^10.2.0",
    "json2typescript": "^1.4.1"
  }
}
